extends layout

append head
  //- add page specific styles by appending to the head
  link(rel="stylesheet" href="/stylesheets/home.css")
  //- add page specific js
  script(src="/javascripts/index.js" type="module" defer)

block content
  h1= title
  p Welcome to your Project Skeleton

  nav
    a(href=("/"))
      img(src="/images/Mudzurilla.JPG")
    select(name="Genre") 
      option select a genre
    select(name="System")
      each system in Systems
        option(value=system.id name="system")= system.system  

